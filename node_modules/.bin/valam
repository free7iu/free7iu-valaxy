#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/blog/free7iu-valaxy/node_modules/.pnpm/valaxy@0.14.17_@types+markdown-it@12.2.3_postcss@8.4.21_typescript@4.8.4/node_modules/valaxy/bin/node_modules:/mnt/d/blog/free7iu-valaxy/node_modules/.pnpm/valaxy@0.14.17_@types+markdown-it@12.2.3_postcss@8.4.21_typescript@4.8.4/node_modules/valaxy/node_modules:/mnt/d/blog/free7iu-valaxy/node_modules/.pnpm/valaxy@0.14.17_@types+markdown-it@12.2.3_postcss@8.4.21_typescript@4.8.4/node_modules:/mnt/d/blog/free7iu-valaxy/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/blog/free7iu-valaxy/node_modules/.pnpm/valaxy@0.14.17_@types+markdown-it@12.2.3_postcss@8.4.21_typescript@4.8.4/node_modules/valaxy/bin/node_modules:/mnt/d/blog/free7iu-valaxy/node_modules/.pnpm/valaxy@0.14.17_@types+markdown-it@12.2.3_postcss@8.4.21_typescript@4.8.4/node_modules/valaxy/node_modules:/mnt/d/blog/free7iu-valaxy/node_modules/.pnpm/valaxy@0.14.17_@types+markdown-it@12.2.3_postcss@8.4.21_typescript@4.8.4/node_modules:/mnt/d/blog/free7iu-valaxy/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../valaxy/bin/valaxy.mjs" "$@"
else
  exec node  "$basedir/../valaxy/bin/valaxy.mjs" "$@"
fi
